{
  "name": "cynosure",
  "version": "1.0.0",
  "description": "Claude Code MAX Bridge - OpenAI API compatible proxy for Claude Code SDK",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "npx tsx watch --clear-screen=false src/index.ts",
    "dev:debug": "npx tsx watch --inspect src/index.ts",
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src tests --ext .ts --cache",
    "lint:fix": "eslint src tests --ext .ts --fix --cache",
    "lint:report": "eslint src tests --ext .ts --format html --output-file eslint-report.html",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.json\" \"*.md\" \"*.yml\" \"*.yaml\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.json\" \"*.md\" \"*.yml\" \"*.yaml\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "clean": "rm -rf dist coverage .tscache eslint-report.html",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "docker:build": "docker build -t cynosure:latest .",
    "docker:build:optimized": "docker build -f Dockerfile.optimized -t cynosure:optimized .",
    "docker:run": "docker run -p 3000:3000 --name cynosure-dev cynosure:latest",
    "docker:run:prod": "docker-compose -f docker-compose.production.yml up -d",
    "docker:stop": "docker stop cynosure-dev && docker rm cynosure-dev",
    "docker:logs": "docker logs -f cynosure-dev",
    "docker:shell": "docker exec -it cynosure-dev /bin/bash",
    "deps:check": "npm outdated",
    "deps:update": "npm update && npm audit fix",
    "deps:audit": "npm audit",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "performance:profile": "NODE_ENV=production node --prof dist/index.js",
    "performance:analyze": "node --prof-process isolate-*.log > profile.txt",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "metrics:prometheus": "curl -H 'Accept: text/plain' http://localhost:3000/metrics",
    "precommit": "npm run lint:fix && npm run format && npm run typecheck && npm run test:unit",
    "prepush": "npm run test && npm run build",
    "ci": "npm run lint && npm run format:check && npm run typecheck && npm run test:coverage && npm run build",
    "ci:fast": "npm run lint && npm run typecheck && npm run test:unit",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "docs:generate": "typedoc src --out docs/api --theme minimal",
    "benchmark": "node scripts/benchmark.js",
    "validate:env": "node -e \"require('./dist/utils/helpers.js').validateEnvironment()\"",
    "migration:create": "node scripts/create-migration.js",
    "setup": "npm install && npm run build && npm run test:unit",
    "setup:dev": "npm install && cp .env.example .env && npm run build"
  },
  "keywords": [
    "claude",
    "openai",
    "api",
    "proxy",
    "bridge",
    "ai",
    "anthropic"
  ],
  "author": "Cynosure Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.0",
    "@anthropic-ai/sdk": "^0.54.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "pino": "^8.19.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
